<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
	updateCheck="true" monitoring="autodetect" dynamicConfig="true">

	<diskStore path="java.io.tmpdir" />


	<defaultCache maxElementsInMemory="10000" eternal="false"
		timeToIdleSeconds="300" timeToLiveSeconds="300" overflowToDisk="true"
		diskSpoolBufferSizeMB="30" maxElementsOnDisk="10000" diskPersistent="false"
		diskExpiryThreadIntervalSeconds="120" memoryStoreEvictionPolicy="LRU"
		statistics="true" />

	<cache name="allPrograms" maxElementsInMemory="20000"
		maxElementsOnDisk="5000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="3600" timeToLiveSeconds="1800"
		memoryStoreEvictionPolicy="LRU" statistics="true" />

	<cache name="userProgramReserveStatus" maxElementsInMemory="20000"
		maxElementsOnDisk="5000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="3600" timeToLiveSeconds="1800"
		memoryStoreEvictionPolicy="LRU" statistics="true" />


	<cache name="programsUserCounts" maxElementsInMemory="20000"
		maxElementsOnDisk="5000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="3600" timeToLiveSeconds="900"
		memoryStoreEvictionPolicy="FIFO" statistics="true" />


	<cache name="userPrograms" maxElementsInMemory="20000"
		maxElementsOnDisk="5000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="3600" timeToLiveSeconds="300"
		memoryStoreEvictionPolicy="LRU" statistics="true" />


	<cache name="liveInfo" maxElementsInMemory="20000"
		maxElementsOnDisk="5000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="3600" timeToLiveSeconds="60"
		memoryStoreEvictionPolicy="LRU" statistics="true" />

	<cache name="idolHeader" maxElementsInMemory="20000"
		maxElementsOnDisk="5000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="3600" timeToLiveSeconds="900"
		memoryStoreEvictionPolicy="LRU" statistics="true" />

	<cache name="idolInfo" maxElementsInMemory="20000"
		maxElementsOnDisk="5000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="3600" timeToLiveSeconds="900"
		memoryStoreEvictionPolicy="LRU" statistics="true" />

	<cache name="vipAndGrade" maxElementsInMemory="20000"
		maxElementsOnDisk="5000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="3600" timeToLiveSeconds="900"
		memoryStoreEvictionPolicy="LRU" statistics="true" />

	<cache name="userFansCount" maxElementsInMemory="20000"
		maxElementsOnDisk="5000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="900" timeToLiveSeconds="900"
		memoryStoreEvictionPolicy="LRU" statistics="true" />

	<cache name="userFlowerCount" maxElementsInMemory="20000"
		maxElementsOnDisk="5000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="900" timeToLiveSeconds="900"
		memoryStoreEvictionPolicy="LRU" statistics="true" />
	<cache name="searchHotWord" maxElementsInMemory="10"
		maxElementsOnDisk="0" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="1800" timeToLiveSeconds="1800"
		memoryStoreEvictionPolicy="LRU" statistics="true" />
	<cache name="historyLiveList" maxElementsInMemory="10"
		maxElementsOnDisk="1" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="1800" timeToLiveSeconds="1800"
		memoryStoreEvictionPolicy="LRU" statistics="true" />
	<cache name="noticeCached" maxElementsInMemory="10000"
		maxElementsOnDisk="10000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="900" timeToLiveSeconds="900"
		memoryStoreEvictionPolicy="LRU" statistics="true" />
	
	<!-- 新导航 -->
	<cache name="entNavigate" maxElementsInMemory="10"
		maxElementsOnDisk="10" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="1800" timeToLiveSeconds="1800"
		memoryStoreEvictionPolicy="LRU" statistics="true" />
		
	<cache name="entBanner" maxElementsInMemory="10"
		maxElementsOnDisk="10" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="2592000" timeToLiveSeconds="2592000"
		memoryStoreEvictionPolicy="LRU" statistics="true" />

	<cache name="entSdBanner" maxElementsInMemory="10"
		   maxElementsOnDisk="10" eternal="false" overflowToDisk="false"
		   timeToIdleSeconds="2592000" timeToLiveSeconds="2592000"
		   memoryStoreEvictionPolicy="LRU" statistics="true" />

	<cache name="entHypertext" maxElementsInMemory="10"
		maxElementsOnDisk="10" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="2592000" timeToLiveSeconds="2592000"
		memoryStoreEvictionPolicy="LRU" statistics="true" />

	<cache name="entRule" maxElementsInMemory="50"
		maxElementsOnDisk="10" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="2592000" timeToLiveSeconds="2592000"
		memoryStoreEvictionPolicy="LRU" statistics="true" />
		
	<!-- 调用出错规避 -->
	<cache name="callIfaInfo" maxElementsInMemory="20"
		maxElementsOnDisk="20" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="300" timeToLiveSeconds="300"
		memoryStoreEvictionPolicy="FIFO" statistics="true" />
		
	<cache name="liveName" maxElementsInMemory="2000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="1800" timeToLiveSeconds="1800"
		memoryStoreEvictionPolicy="FIFO" statistics="true" />
		
	<cache name="liveNameStyle" maxElementsInMemory="2000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="3600" timeToLiveSeconds="3600"
		memoryStoreEvictionPolicy="FIFO" statistics="true" />
		
	<!-- 艺名缓存 -->
	<cache name="artistName" maxElementsInMemory="100000"
		maxElementsOnDisk="100000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="86400" timeToLiveSeconds="86400"
		memoryStoreEvictionPolicy="LRU" statistics="true" />
		
	<!-- 直播间描述 -->
	<cache name="channelDesc" maxElementsInMemory="100000"
		maxElementsOnDisk="100000" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="86400" timeToLiveSeconds="86400"
		memoryStoreEvictionPolicy="LRU" statistics="true" />	

	<!-- 4.x首页专题缓存 -->
	<cache name="indexTopic" maxElementsInMemory="100000"
		   maxElementsOnDisk="100000" eternal="false" overflowToDisk="false"
		   timeToIdleSeconds="300" timeToLiveSeconds="2592000"
		   memoryStoreEvictionPolicy="LRU" statistics="true" />

	<!-- 直播模块缓存 -->
	<cache name="entLiveModule" maxElementsInMemory="100000"
		   maxElementsOnDisk="100000" eternal="false" overflowToDisk="false"
		   timeToIdleSeconds="300" timeToLiveSeconds="2592000"
		   memoryStoreEvictionPolicy="LRU" statistics="true" />

	<!--渠道问候语缓存-->
	<cache name="entChannelGreeting" maxElementsInMemory="100000"
		   maxElementsOnDisk="100000" eternal="false" overflowToDisk="false"
		   timeToIdleSeconds="300" timeToLiveSeconds="2592000"
		   memoryStoreEvictionPolicy="LRU" statistics="true" />

	<!--渠道默认tab缓存-->
	<cache name="entChannelDefaultNav" maxElementsInMemory="100000"
		   maxElementsOnDisk="100000" eternal="false" overflowToDisk="false"
		   timeToIdleSeconds="300" timeToLiveSeconds="2592000"
		   memoryStoreEvictionPolicy="LRU" statistics="true" />

	<!--小米送礼提示语缓存-->
	<cache name="entMiReminder" maxElementsInMemory="100000"
		   maxElementsOnDisk="100000" eternal="false" overflowToDisk="false"
		   timeToIdleSeconds="300" timeToLiveSeconds="2592000"
		   memoryStoreEvictionPolicy="LRU" statistics="true" />

	<!--版本升级库缓存-->
	<cache name="entSdkVersion" maxElementsInMemory="1000"
		   maxElementsOnDisk="1000" eternal="false" overflowToDisk="false"
		   timeToIdleSeconds="300" timeToLiveSeconds="2592000"
		   memoryStoreEvictionPolicy="LRU" statistics="true" />
		   
	<!--5.0 yy.com导航-->
	<cache name="entYycomNav" maxElementsInMemory="100000"
		   maxElementsOnDisk="100000" eternal="false" overflowToDisk="false"
		   timeToIdleSeconds="300" timeToLiveSeconds="2592000"
		   memoryStoreEvictionPolicy="LRU" statistics="true" />

	<!--5.0 一级分类 -->
	<cache name="entV5TopNav" maxElementsInMemory="100000"
		   maxElementsOnDisk="100000" eternal="false" overflowToDisk="false"
		   timeToIdleSeconds="300" timeToLiveSeconds="2592000"
		   memoryStoreEvictionPolicy="LRU" statistics="true" />

	<!--5.0 小幅通栏 -->
	<cache name="entSmallBanner" maxElementsInMemory="100000"
		   maxElementsOnDisk="100000" eternal="false" overflowToDisk="false"
		   timeToIdleSeconds="300" timeToLiveSeconds="2592000"
		   memoryStoreEvictionPolicy="LRU" statistics="true" />
		   
   	<!-- 运营标题-->
	<cache name="entLiveTitle" maxElementsInMemory="10"
		maxElementsOnDisk="10" eternal="false" overflowToDisk="false"
		timeToIdleSeconds="2592000" timeToLiveSeconds="2592000"
		memoryStoreEvictionPolicy="LRU" statistics="true" />

	<!-- 运营标签-->
	<cache name="entLiveTag" maxElementsInMemory="10"
		   maxElementsOnDisk="10" eternal="false" overflowToDisk="false"
		   timeToIdleSeconds="2592000" timeToLiveSeconds="2592000"
		   memoryStoreEvictionPolicy="LRU" statistics="true" />

	<!-- 运营封面-->
	<cache name="entLiveThumb" maxElementsInMemory="10"
		   maxElementsOnDisk="10" eternal="false" overflowToDisk="false"
		   timeToIdleSeconds="2592000" timeToLiveSeconds="2592000"
		   memoryStoreEvictionPolicy="LRU" statistics="true" />

	<!--直播内通栏-->
	<cache name="entSmallBannerRel" maxElementsInMemory="10"
		   maxElementsOnDisk="10" eternal="false" overflowToDisk="false"
		   timeToIdleSeconds="2592000" timeToLiveSeconds="2592000"
		   memoryStoreEvictionPolicy="LRU" statistics="true" />
</ehcache>